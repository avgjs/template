{
    "sourceMaps": true,
    "comments": false,
    "presets": [
        [
            "latest",
            {
                "es2015": {
                    "modules": false
                }
            }
        ],
        "react"
    ],
    "plugins": [
        "react-hot-loader/babel",
        "transform-decorators-legacy",
        "transform-class-properties",
        "transform-export-extensions",
        [
            "transform-object-rest-spread",
            {
                "useBuiltIns": true
            }
        ]
    ]
}